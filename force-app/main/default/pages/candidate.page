<apex:page StandardController="Candidate__c" lightningStyleSheets="true">

    <apex:form >

        <apex:pageBlock title="Candidate Entry Form" >

            <apex:pageBlockButtons location="bottom">

                <apex:commandButton action="{!save}" value="Save"/>

            </apex:pageBlockButtons>

            <apex:pageBlockSection>
              
                    
                <apex:inputField value="{!Candidate__c.name}"/>
                <apex:inputField value="{!Candidate__c.College__c}"/>
                <apex:inputField value="{!Candidate__c.Branch__c}"/>
                <apex:inputField value="{!Candidate__c.Pass_Out_Year__c}"/>
                <apex:inputField value="{!Candidate__c.CGPA__c} "/>              
                <apex:inputField value="{!Candidate__c.Date_of_birth__c}" label="DOB"/>
                <apex:inputField value="{!Candidate__c.Phone__c}"/>
                <apex:inputField value="{!Candidate__c.Email__c}"/>
                
                
            </apex:pageBlockSection>          
        </apex:pageBlock>
    </apex:form>
</apex:page>